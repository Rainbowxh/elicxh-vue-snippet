{
  "Vue composition template": {
    "prefix": ["vue.template", "vtemplate"],
    "body": [
      "<template>",
      "  <div>",
      "    $1",
      "  </div>",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "$2",
      "</script>",
      "",
      "<style scoped>",
      "$3",
      "</style>"
    ]
  },
  "Vue ref": {
    "prefix": ["vref"],
    "body": "const $1 = ref($2);",
    "description": "[Vue ref documentation](https://v3.vuejs.org/api/refs-api.html#ref)",
    "scope": "typescript"
  }
}


